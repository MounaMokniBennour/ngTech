{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "apiPersonne",
      "urls": [
        "http://localhost:3000/api/personnes"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "2d",
        "timeout": "5s",
        "strategy": "freshness"
      }
    }
  ]
}
